{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}



<div class="container">

    <form{{ attributes }}>
    {{ form.form_build_id }}
    {{ form.form_token }}
    {{ form.form_id }}
        <div class="row">
            <div class="col-lg-6 col-sm-12">
                {{ form.quantity }}
            </div>
            <div class="col-lg-6 col-sm-12">
                {{ form.district }}
            </div>
            <div class="col-lg-6 col-sm-12">
                {{ form.phone }}
        {{ form.address }}
        {{ form.price }}
        {{ form.submit }}
            </div>
        </div>

    </form>
</div>